[user]
    name = Matt Waggoner
    email = matt@mattwaggs.com
[core]
    editor = vim
[alias]
    s = status
    stauts = status # because typos ¯\_(ツ)_/¯
    c = commit
    co = checkout
    cm = commit -m
    dw = diff -w
    pom = push origin master
    lg = log --graph --abbrev-commit -C -M --pretty=format:\"%Cred%h%Creset %ar by %C(yellow)%an%Creset %C(auto)%d %s\" --all
    new = log origin/master.. --reverse -C -M --pretty=format:\"<%Cred%h%Creset> %ad [%C(yellow)%an%Creset] %Cgreen%d%Creset %s\" --all --date=short
    pick = cherry-pick
    cleanup = "!git branch --merged | grep -v '\\*\\|master\\|dev' | xargs -n 1 git branch -d"
[color]
    ui = auto
[diff]
    algorithm = patience
